{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Analytics-ADF"
		},
		"AmazonS3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS3'"
		},
		"AzureBlobStorageAWSTOAZURE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageAWSTOAZURE'"
		},
		"GoogleCloudStorage1 S3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'GoogleCloudStorage1 S3'"
		},
		"LKS_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LKS_AzureDataLakeStorage'"
		},
		"LKS_Azure_SQL_SERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LKS_Azure_SQL_SERVER'"
		},
		"LS_MySqlAWS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_MySqlAWS'"
		},
		"AmazonS3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAQGAW7RYO5WPBMHHO"
		},
		"GoogleCloudStorage1 S3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOGOIETTMSUAIELDL6EWJU4"
		},
		"HttpServerBrasilIO_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.brasil.io/"
		},
		"LKS_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagelakegen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Analytics-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS3_secretAccessKey": {
						"value": "[parameters('AmazonS3_secretAccessKey')]"
					},
					"AzureBlobStorageAWSTOAZURE_connectionString": {
						"value": "[parameters('AzureBlobStorageAWSTOAZURE_connectionString')]"
					},
					"GoogleCloudStorage1 S3_secretAccessKey": {
						"value": "[parameters('GoogleCloudStorage1 S3_secretAccessKey')]"
					},
					"LKS_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('LKS_AzureDataLakeStorage_accountKey')]"
					},
					"LKS_Azure_SQL_SERVER_connectionString": {
						"value": "[parameters('LKS_Azure_SQL_SERVER_connectionString')]"
					},
					"AmazonS3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3_properties_typeProperties_accessKeyId')]"
					},
					"GoogleCloudStorage1 S3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('GoogleCloudStorage1 S3_properties_typeProperties_accessKeyId')]"
					},
					"HttpServerBrasilIO_properties_typeProperties_url": {
						"value": "[parameters('HttpServerBrasilIO_properties_typeProperties_url')]"
					},
					"LKS_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LKS_AzureDataLakeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Analytics-ADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_MySqlAWS_connectionString": {
						"value": "[parameters('LS_MySqlAWS_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Analytics-ADF_ArmTemplate_0')]"
			]
		}
	]
}